{
	"name": "@werstiehltihrdieshow/frontend",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "wrangler dev .svelte-kit/cloudflare/_worker.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"types": "wrangler types --env dev",
		"deploy": "pnpm build && wrangler deploy",
		"prepare": "pnpm run types && svelte-kit sync || echo ''"
	},
	"devDependencies": {
		"@iconify-json/mdi": "1.2.3",
		"@sveltejs/adapter-cloudflare": "7.2.4",
		"@sveltejs/kit": "2.49.2",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"svelte": "5.46.1",
		"svelte-check": "4.3.5",
		"typescript": "5.9.3",
		"unocss": "66.5.12",
		"unplugin-icons": "22.5.0",
		"vite": "8.0.0-beta.5",
		"vite-plugin-devtools-json": "1.0.0",
		"wrangler": "4.56.0"
	},
	"dependencies": {
		"@ai-sdk/openai": "3.0.2",
		"@werstiehltihrdieshow/auth": "workspace:*",
		"@werstiehltihrdieshow/db": "workspace:*",
		"ai": "6.0.6",
		"bits-ui": "2.15.2",
		"openai": "6.15.0",
		"valibot": "1.2.0"
	}
}
